package net.cybertristam.presentation;

import net.cybertristam.business.StatisticsToolMgr;
import net.cybertristam.dataObjects.Statistics;

import java.awt.event.ActionListener;
import java.awt.event.ActionEvent;
import java.awt.*;
import java.io.File;

public class StatisticsToolController implements ActionListener {
    StatisticsToolManagerUI stm;
    public StatisticsToolController() {

    }

    public StatisticsToolController(StatisticsToolManagerUI stm){
        this.stm = stm;
    }


    public void actionPerformed(ActionEvent e) {
        //String separator = File.separator;
        String separator = "\n";
        System.out.println("Separator value: " + separator);
        StatisticsToolMgr st = new StatisticsToolMgr();
        System.out.println("Command is: " + e.getActionCommand());
        Statistics enteredStats = stm.getData();
        Long totalUpTime = st.determineServerTCPPortUpTime(enteredStats.getAddress(),enteredStats.getPortNumber());
        String resultsText = "Hostname: " + enteredStats.getAddress() + separator;
        resultsText += "Port Number: " + enteredStats.getPortNumber() + separator;
        resultsText += "Up Time: " + totalUpTime + " milliseconds" + separator;
        stm.setResults(resultsText);
        //To change body of implemented methods use File | Settings | File Templates.
    }
}
